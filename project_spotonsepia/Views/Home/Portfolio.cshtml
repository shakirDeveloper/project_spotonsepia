    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/css/vendor/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/css/vendor/slick.css" rel="stylesheet" />
    <link href="~/Content/css/vendor/slick-theme.css" rel="stylesheet" />
    <link href="~/Content/css/vendor/sal.css" rel="stylesheet" />
    <link href="~/Content/css/vendor/magnific-popup.css" rel="stylesheet" />
